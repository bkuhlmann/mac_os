#! /usr/bin/env bash

# Installs Node packages.

set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'

SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/install_node_packages"

if [[ -x "$SCRIPT_PATH" ]]; then
  install_node
  "$SCRIPT_PATH"
else
  printf "WARNING: Node packages install script does not exist or is not executable.\n"
fi
