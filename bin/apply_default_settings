#! /usr/bin/env bash

# DESCRIPTION
# Applies default settings.

# SETTINGS
set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'
SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/apply_default_settings"

# EXECUTION
if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: Default settings script does not exist or is not executable.\n"
fi
