#! /usr/bin/env bash

# Installs App Store software.

set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'

SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/install_app_store"

if [[ -x "$SCRIPT_PATH" ]]; then
  check_mas_install
  "$SCRIPT_PATH"
else
  printf "WARNING: App Store install script does not exist or is not executable.\n"
fi
