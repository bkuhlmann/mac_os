#! /usr/bin/env bash

# Installs basic system settings.

set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'

SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/install_basics"

if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "%s\n" "WARNING: Basic settings script does not exist or is not executable."
fi
