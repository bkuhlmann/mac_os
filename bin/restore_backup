#! /usr/bin/env bash

# DESCRIPTION
# Performs restoration of backup data.

# SETTINGS
set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'
SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/restore_backup"

# EXECUTION
if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: Restore backup script does not exist or is not executable.\n"
fi
